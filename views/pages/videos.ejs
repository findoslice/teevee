<!DOCTYPE html>

<html>
	<head>
        <%- include('../partials/head.ejs'); %>
	</head>
    <body>
        <div class="container">
            <p>available videos: </p>
            <ul class="video-list">
                <% for (let video of videos) { %>
                    <li class="video-list__item">
                        <a href="/watch/<%= video %>"><%= video %></a>
                    </li>
                <% } %>
            </ul>
        </div>
    </body>
</html>
